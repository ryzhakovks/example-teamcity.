<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="eebd2f97-911b-4491-b1a2-7cc524fda8eb" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Build</name>
  <description />
  <settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_1" name="Maven for master-branch" type="Maven2">
        <conditions>
          <equals name="teamcity.build.branch" value="master" />
        </conditions>
        <parameters>
          <param name="goals" value="clean deploy" />
          <param name="localRepoScope" value="agent" />
          <param name="maven.path" value="%teamcity.tool.maven.DEFAULT%" />
          <param name="pomLocation" value="pom.xml" />
          <param name="runnerArgs" value="-Dmaven.test.failure.ignore=true" />
          <param name="teamcity.step.mode" value="default" />
          <param name="userSettingsSelection" value="settings.xml" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="Maven for nomaster-branch (test)" type="Maven2">
        <conditions>
          <does-not-equal name="teamcity.build.branch" value="master" />
        </conditions>
        <parameters>
          <param name="goals" value="clean deploy" />
          <param name="localRepoScope" value="agent" />
          <param name="maven.path" value="%teamcity.tool.maven.DEFAULT%" />
          <param name="pomLocation" value="pom.xml" />
          <param name="runnerArgs" value="-Dmaven.test.failure.ignore=true" />
          <param name="teamcity.step.mode" value="default" />
          <param name="userSettingsSelection" value="settings.xml" />
        </parameters>
      </runner>
      <runner id="RUNNER_3" name="Hellooo World (test export to Git)" type="simpleRunner">
        <parameters>
          <param name="script.content" value="echo &quot;Hello World&quot;" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Netology_GitGithubComAirDRoNLabExampleTeamcityGitRefsHeadsMaster" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="TRIGGER_1" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:*" />
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <cleanup />
  </settings>
</build-type>

